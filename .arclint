{
  "linters": {
    "pylint": {
      "type": "pylint",
      "pylint.config": ".pylintrc",
      "include": "(\\.py$)"
    },
    "black": {
      "type": "black",
      "include": "(\\.py$)",
      "black.linelen": 100
    },
    "eslint": {
      "type": "eslint",
      "include": "(\\.(ts|tsx)$)",
      "bin": "./js_modules/dagit/node_modules/.bin/eslint",
      "eslint.config": "./js_modules/dagit/.eslintrc.js"
    }
  }
}
