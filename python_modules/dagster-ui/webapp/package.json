    "@vx/network": "^0.0.170",
    "@vx/responsive": "^0.0.165",
    "@vx/scale": "^0.0.165",
    "@vx/shape": "^0.0.170",
    "apollo-boost": "^0.1.12",
    "d3-hierarchy": "^1.1.6",
    "graphql-tag": "^2.9.2",
    "react": "^16.4.2",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
