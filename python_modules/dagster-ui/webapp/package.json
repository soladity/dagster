    "babel-plugin-graphql-tag": "^1.6.0",
    "graphql": "^0.13.2",
    "prettier": "^1.14.0",
    "react-scripts-ts": "2.16.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "download-schema": "apollo schema:download --endpoint $REACT_APP_GRAPHQL_URI ./schema.json",
    "generate-types": "apollo codegen:generate --queries \"./src/**/*.tsx\" --target typescript types --schema ./schema.json",
    "watch-types": "apollo codegen:generate --queries \"./src/**/*.tsx\" --target typescript types --schema ./schema.json --watch"
  }
