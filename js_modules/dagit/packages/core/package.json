  "scripts": {
    "preinstall": "node src/scripts/yarnCheck.js",
    "prettier": "prettier --write \"./src/**/*.tsx\"",
    "find-dead-code": "yarn ts-prune | grep -v '/types/\\|stories.tsx\\|src/main.tsx'",
    "test": "yarn ts && yarn jest --watchAll=false && yarn lint",
    "jest": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "jest:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "lint": "eslint src/ --ext=.tsx,.ts,.js --fix -c .eslintrc.js",
    "check-lint": "eslint src/ --ext=.tsx,.ts,.js",
    "check-prettier": "prettier --list-different \"./src/**/*.tsx\"",
    "ts": "tsc -p .",
    "download-schema": "ts-node -O '{\"module\": \"commonjs\"}' ./src/scripts/DownloadSchema.ts",
    "generate-types": "find src -type d -name types | xargs rm -r && apollo codegen:generate --includes \"./src/**/*.tsx\" --target typescript types --localSchemaFile ./src/graphql/schema.graphql --globalTypesFile ./src/types/globalTypes.ts && python append_generated.py",
    "watch-types": "apollo codegen:generate --includes \"./src/**/*.tsx\" --target typescript types --localSchemaFile ./src/graphql/schema.graphql  --globalTypesFile ./src/types/globalTypes.ts --watch",
    "storybook": "start-storybook -p 6006 -s lib",
    "build-storybook": "build-storybook -s lib",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/jest/fileTransformer.js"
    },
    "moduleNameMapper": {
      "^worker-loader(.*)/workers/(.*)$": "<rootDir>/jest/mocks/$2"
    }
  },
  "babelMacros": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}